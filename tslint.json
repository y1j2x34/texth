{
    "extends": [
        "tslint:latest",
        "tslint-config-prettier"
    ],
    "rulesDirectory": [
        "tslint-plugin-prettier"
    ],
    "rules": {
        "object-literal-sort-keys": false,
        "no-var-require": false,
        "no-console": false,
        "no-bitwise": false,
        "ordered-imports": false,
        "interface-name": false,
        "max-classes-per-file": false,
        "no-implicit-dependencies": false,
        "prettier": true,
        "no-namespace": false
    }
}
